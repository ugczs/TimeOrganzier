<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.saradruid.yuguanzhao.timeorganzier">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_l"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_r"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.saradruid.yuguanzhao.timeorganizier.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.saradruid.yuguanzhao.timeorganizier.DatePicker" />
        <activity android:name=".HourPicker" />
        <activity android:name="com.saradruid.yuguanzhao.timeorganizier.TimePicker"></activity>
    </application>

</manifest>